{
    "name": "agecalc/age-calc",
    "type": "typo3-cms-extension",
    "description": "Advanced age calculator to fix problems with singular/plural grammatical forms in age.",
    "authors": [
        {
            "name": "Aashro Tech",
            "email": "info@aashro.com",
            "role": "Developer",
            "homepage": "http://www.aashro.com/"
        }
    ],
    "license": "GPL-2.0-or-later",
    "require": {
        "typo3/cms-core": "^10.4 || ^11.5 || ^12.4"
    },
    "autoload": {
        "psr-4": {
            "AgeCalc\\AgeCalc\\": "Classes"
        }
    },
    "replace": {
        "typo3-ter/age-calc": "self.version"
    },
    "config": {
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin"
    },
    "extra": {
        "typo3/cms": {
            "web-dir": ".Build/public",
            "extension-key": "at_agecalc"
        }
    }
}